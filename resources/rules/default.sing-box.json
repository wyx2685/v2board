{
    "dns": {
        "rules": [
            {
                "outbound": [
                    "any"
                ],
                "server": "local"
            }, 
            {
                "clash_mode": "جهانی",
                "server": "remote"
            },
            {
                "clash_mode": "مستقیم",
                "server": "local"
            },
            {
                "rule_set": [
                    "geosite-ir"
                ],
                "server": "local"
            },
            {
                "rule_set": [
                    "category-ads-all"
                ],
                "server": "block"
            }
        ],
        "servers": [
            {
                "address": "https://1.1.1.1/dns-query",
                "detour": "دکتر‌موبایل‌جایزان",
                "tag": "remote"
            },
            {
                "address": "8.8.8.8",
                "detour": "direct",
                "tag": "local"
            },
            {
                "address": "rcode://success",
                "tag": "block"
            }
        ],
        "strategy": "ipv4_only"
    },
    "experimental": {
        "cache_file": {
            "enabled": true
        },
        "clash_api": {
            "default_mode": "قوانین",
            "external_controller": "127.0.0.1:9090",
            "secret": ""
        }
    },
    "inbounds": [
        {
            "auto_route": true,
            "domain_strategy": "prefer_ipv4",
            "endpoint_independent_nat": true,
            "inet4_address": "172.19.0.1/30",
            "inet6_address": "2001:0470:f9da:fdfa::1/64",
            "mtu": 9000,
            "sniff": true,
            "sniff_override_destination": true,
            "stack": "system",
            "strict_route": true,
            "type": "tun"
        },
        {
            "domain_strategy": "prefer_ipv4",
            "listen": "127.0.0.1",
            "listen_port": 2333,
            "sniff": true,
            "sniff_override_destination": true,
            "tag": "socks-in",
            "type": "socks",
            "users": []
        },
        {
            "domain_strategy": "prefer_ipv4",
            "listen": "127.0.0.1",
            "listen_port": 2334,
            "sniff": true,
            "sniff_override_destination": true,
            "tag": "mixed-in",
            "type": "mixed",
            "users": []
        }
    ],
    "outbounds": [
        {
            "type": "selector",
            "tag": "دکتر‌موبایل‌جایزان",
            "outbounds": [
                "انتخاب خودکار"
            ]
        },
        {
            "type": "urltest",
            "tag": "انتخاب خودکار",
            "outbounds": []
        },
        {
            "type": "dns",
            "tag": "dns-out"
        },
        {
            "type": "direct",
            "tag": "direct"
        },
        {
            "type": "block",
            "tag": "block"
        }
    ],
    "route": {
        "auto_detect_interface": true,
        "rules": [
            {
                "outbound": "dns-out",
                "protocol": "dns"
            },
            {
                "clash_mode": "مستقیم",
                "outbound": "direct"
            },
            {
                "clash_mode": "جهانی",
                "outbound": "دکتر‌موبایل‌جایزان"
            },
            {
                "rule_set": [
                    "geosite-ir",
                    "geoip-ir"
                ],
                "outbound": "direct"
            },
            {
                "ip_is_private": true,
                "outbound": "direct"
            },
            {
                "rule_set": [
                    "category-ads-all"
                ],
                "outbound": "block"
            }
        ],
        "rule_set": [
            {
                "tag": "geosite-ir",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/Chocolate4U/Iran-sing-box-rules/rule-set/geosite-ir.srs",
                "download_detour": "دکتر‌موبایل‌جایزان"
            },
            {
                "tag": "category-ads-all",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/SagerNet/sing-geosite/rule-set/geosite-category-ads-all.srs",
                "download_detour": "دکتر‌موبایل‌جایزان"
            },
            {
                "tag": "geoip-ir",
                "type": "remote",
                "format": "binary",
                "url": "https://raw.githubusercontent.com/Chocolate4U/Iran-sing-box-rules/rule-set/geoip-ir.srs",
                "download_detour": "دکتر‌موبایل‌جایزان"
            }
        ]
    }
}
